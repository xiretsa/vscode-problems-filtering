    // Configuration pour Coverage Gutters
    "coverage-gutters.coverageFileNames": [
        "lcov.info",
        "coverage.json",
        "cov.xml",
        "coverage.xml"
    ],
    "coverage-gutters.coverageReportFileName": "index.html",
    "coverage-gutters.coverageBaseDir": "target/coverage",
